<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="editContinentPane" prefHeight="200.0" prefWidth="1500.0" style="-fx-background-color: lightGray;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContinentController">
   <children>
      <Label layoutX="50.0" layoutY="20.0" prefHeight="30.0" prefWidth="150.0" text="Continent Name" />
      <Label layoutX="250.0" layoutY="20.0" prefHeight="30.0" prefWidth="150.0" text="Continent Value" />
      <TextField fx:id="addContinentName" layoutX="50.0" layoutY="50.0" prefWidth="150.0" />
      <TextField fx:id="continentValue" layoutX="250.0" layoutY="50.0" prefWidth="150.0" />
      <Button fx:id="addContinentButton" layoutX="446.0" layoutY="49.0" mnemonicParsing="false" onAction="#addContinent" prefHeight="30.0" prefWidth="60.0" text="Add" />
      <TextField fx:id="delContinentName" layoutX="50.0" layoutY="107.0" prefWidth="150.0" />
      <Button fx:id="deleteContinentButton" layoutX="446.0" layoutY="106.0" mnemonicParsing="false" onAction="#deleteContinent" prefHeight="30.0" prefWidth="60.0" text="Delete" />
   </children>
</Pane>
